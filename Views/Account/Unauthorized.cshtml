
@{
    ViewBag.Title = "Unauthorized";
}

<section class="content">
    <div class="box box-info">
        <div class="box-body">
            <div class="table-responsive">
                <div class="col-md-12">
                    <div class="error-template text-center">
                        <h1 class="text-primary">
                            Oops!
                        </h1>
                        <h2 class="text-danger">
                            401, Access Denied
                        </h2>
                        <div class="text-light">
                            Sorry, your are not authorized to view the page!
                        </div>
                        <div class="form-group pad10">
                            @Html.ActionLink("<< Back to Home ", "Index", "Home", null, new { @class = "btn btn-gradyellow btn-flat btnalign" })
                        </div>
                    </div>


                </div>
            </div>

        </div>
    </div>
</section>



